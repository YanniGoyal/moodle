{{#pagination}}
    <nav aria-label="Page navigation" id="page_co_over" value={{{value}}}>
        <ul class="pagination justify-content-center" id="pagination">
            <li class="page-item" id="previous_co_over"><a class="page-link" href="#" tabindex="-1">Previous</a></li>
            {{#pages}}
                <li class="page-item" id="{{{index}}}_co_over"><a class="page-link" href="#">{{{index}}}</a></li>
            {{/pages}}
            <li class="page-item" id="next_co_over"><a class="page-link" href="#" tabindex="-1">Next</a></li>
        </ul>
    </nav>
{{/pagination}}